<script>
  getweb();
  function getweb(){
    if(!!window.ActiveXObject || "ActiveXObject" in window){//判断IE .不支持 cube 动画样式;
      jQuery("#wowslider-container0").wowSlider({effect:"louvers,brick",prev:"",next:"",duration:20*100,delay:45*100,width:640,height:360,autoPlay:true,autoPlayVideo:false,playPause:true,stopOnHover:false,loop:false,bullets:1,caption:true,captionEffect:"parallax",controls:true,controlsThumb:false,responsive:3,fullScreen:false,gestures:2,onBeforeStep:0,images:0});
    }else {
      jQuery("#wowslider-container0").wowSlider({effect:"louvers,brick,cube",prev:"",next:"",duration:20*100,delay:45*100,width:640,height:360,autoPlay:true,autoPlayVideo:false,playPause:true,stopOnHover:false,loop:false,bullets:1,caption:true,captionEffect:"parallax",controls:true,controlsThumb:false,responsive:3,fullScreen:false,gestures:2,onBeforeStep:0,images:0});
    }
    /*if (navigator.userAgent.indexOf("Firefox")!=-1){
      console.log("Firefox");
    };
    if(navigator.userAgent.indexOf("Chrome")!=-1){
      console.log("Chrome");
    };*/
  };
</script>
